<div class="header block">
	<img class="logo" src="" alt="" data-bind="attr: { src:state_logo_url }"/>
	от <span class="sender-name" data-bind="text:title"></span>
</div>

<div class="block">
	<div class="flexbox">
		<span class="caption">Дата и время</span>

		<div class="datetime" data-bind="text:state_datetime"></div>
	</div>
</div>

<div class="block">
	<div class="flexbox">
		<span class="caption">Сумма счета</span>

		<div class="total">
			<span data-bind="text:money(transaction_amount)"></span>
			<span class="rouble">:</span>
		</div>
	</div>
</div>

<div class="block">
	<div class="flexbox">
		<span class="caption">CashBack</span>
		<span class="cashback-percent">
			<span data-bind="text:cashbackTemplate_cashback"></span>%
		</span>

		<div class="cashback-amount">
			<span data-bind="text:money(amount)"></span>
			<span class="rouble">:</span>
		</div>
	</div>
	<div class="cashback-template-name" data-bind="text:cashbackTemplate_title"></div>
</div>

<div class="block">
	<div class="flexbox">
		<span class="caption">Место</span>

		<div class="company-name" data-bind="text:company_name"></div>
	</div>
	<div class="address-name" data-bind="text:address_address"></div>
</div>

<div class="block">
	<div class="flexbox">
		<span class="caption">Карта</span>

		<div class="card-number" data-bind="text:card_card_number"></div>
	</div>
	<div class="card-name" data-bind="text:card_title"></div>
	<div class="payment-system" data-bind="text:paymentSystem_title"></div>
	<div class="bank-name" data-bind="text:bank_name"></div>
</div>